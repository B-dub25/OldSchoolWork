#include "phoneNumber.h"



int main(int argc, char **argv) {

	phoneNumber myNumber;

	char a = 4;
	if(a -'0' > 2)
		cout << "okay";
	do {
	   try{
		cout << "Enter your phone number ";
		   cin >> myNumber;
		cout << myNumber;
		break;
	   }catch (invalidFormat& e) {
		   cin.clear();
		   cin.ignore(10,'\n');

	}

	} while (true);
}




