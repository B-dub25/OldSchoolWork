#include "phoneNumber.h"



int main(int argc, char **argv) {

	phoneNumber myNumber;

	do {
	   try{
		cout << "Enter your phone number ";
		   cin >> myNumber;
		cout << myNumber;
		break;
	   }catch (exception& e) {
		   cin.clear();
		   cin.ignore(10,'\n');
		  cout << e.what() << endl;
	}

	} while (true);
}




