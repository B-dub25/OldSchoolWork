#include "phoneNumber.h"



int main(int argc, char **argv) {

	phoneNumber myNumber;
	char a[2] = {'1','2'};
	if(a[2] == 2)
		cout << 2;
	do {
	   try{
		cout << "Enter your phone number ";
		   cin >> myNumber;

		cout << myNumber;
		break;
	   }catch (invalidFormat& e) {
		  cout << e.what() << endl;;

	}

	} while (true);
}




