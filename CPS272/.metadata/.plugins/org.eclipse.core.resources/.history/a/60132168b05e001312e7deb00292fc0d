using namespace std;

template<typename K,typename T>
class hashing {
public:
      node<T>* find(K);
      node<T>* operator[](K key){
    	  return list[hash(key)];
      }

private:
       int hash(K);
       miniList<K,T>* list[];
};
template<typename K, typename T>
int hashing<K,T>::hash(K){



}



