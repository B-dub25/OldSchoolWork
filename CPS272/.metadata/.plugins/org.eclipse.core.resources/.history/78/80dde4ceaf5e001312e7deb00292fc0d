#ifndef HASHING_H_
#define HASHING_H_
#include "miniList.h"
template<typename K,typename T>
class hashing {
public:
      node<T>* find(T);
      node<T>* operator[](K key){
    	  return list[hash(key)];
      }

private:
       int hash(T);
       miniList<K,T>* list[];
};




#endif /* HASHING_H_ */
